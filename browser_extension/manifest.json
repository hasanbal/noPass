{
  "manifest_version": 2,
  "name": "QRextension",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "tabs",
    "<all_urls>",
    "notifications",
    "storage",
    "https://get.requestcatcher.com/*",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["jquery.js", "qrcode.js", "popup.js"]
  }
}